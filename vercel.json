{
  "version": 2,
  "builds": [
    {
      "src": "backend.js",
      "use": "@vercel/node"
    },
    {
      "src": "views/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "views/dashboard.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/dashboard.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/favicon.ico",
      "use": "@vercel/static"
    },
    {
      "src": "public/app.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
  {
    "src": "/api/(.*)",
    "dest": "/backend.js"
  },
  {
    "src": "/(.*)",
    "dest": "/views/index.html"
  }
  ],
