{
  "version": 2,
  "builds": [
    {
      "src": "backend.js",
      "use": "@vercel/node"
    },
    {
      "src": "views/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "views/dashboard.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend.js"
    },
    {
      "src": "/(.*)",
      "dest": "/views/index.html"
    }
  ],
  "env": {
    "MONGODB_URI": "mongodb+srv://NotSmellyCat:NSC2000@notsmellycat.u11lpgy.mongodb.net/NCS"
  }
}
